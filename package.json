{
  "name": "proyecto2",
  "version": "1.0.0",
  "main": "dist/server/server/server.js",
  "scripts": {
    "dev:backend": "ts-node-dev --respawn --transpile-only --project tsconfig.backend.json backend/server/server.ts",
    "dev:frontend": "webpack serve --config webpack.config.js --mode development --port 4000",
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "debug": "pwd && ls -la && ls -la frontend/ && cat webpack.config.js | head -20",
    "clean": "rm -rf dist || rmdir /s /q dist",
    "build": "yarn clean && yarn debug && yarn build:backend && yarn build:frontend",
    "build:frontend": "webpack --mode production",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start": "node dist/server/server/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "resend": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "ts-node-dev": "^2.0.0",
    "concurrently": "^8.2.2"
  }
}